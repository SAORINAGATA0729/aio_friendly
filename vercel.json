{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!css|js|data|images|fonts|.*\\.(css|js|json|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
